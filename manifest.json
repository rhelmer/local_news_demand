{
  "description": "Local News Demand Study",
  "author": "Stanford GSB",
  "manifest_version": 2,
  "name": "Local News Demand Study",
  "version": "0.1",
  "homepage_url": "https://github.com/bchivers-stanford/local_news_demand",

  "permissions": [
    "<all_urls>",
    "idle",
    "notifications",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webRequest",
    "alarms",
    "telemetry",
    "history",
    "webNavigation",
    "webRequestBlocking"
  ],
  "browser_action": {},
  "content_security_policy": "script-src 'self' 'sha256-csyiOLMfXk2f5pU99mqYFyshgnVYbdp6o9bnQ9hntPA=' 'sha256-nYNRfLKTaKqgi4+CK/mcp9hdSsmD8F17GWuo+vQGfqU='; object-src 'self'",
  "background": {
    "scripts": ["dist/background.js"]
  }
}
